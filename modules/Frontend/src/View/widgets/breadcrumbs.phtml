<?php

if (! isset($breadcrumbs) || ! is_array($breadcrumbs)) {
    echo 'Param breadcrumbs missing!';
    return;
}

?>

<ul>

    <?php

    foreach ($breadcrumbs as $breadcrumb) {
        ?>
        <li><a href="<?php echo $breadcrumb['link']; ?>"><?php echo $breadcrumb['name']; ?></a></li>
        <?php
    }

    ?>
</ul>
