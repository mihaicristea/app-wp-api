<?php

if (! isset($posts)) {
    return 'Post data missing!';
}

$view->renderWidget([
    'widget' => \Frontend\Widget\Breadcrumbs::class,
    'method' => 'getPostBreadcrumbs',
    'params' => [
        'post' => $posts
    ],
]);

if (! isset($posts->content->rendered)) {
    return 'Post content missing!';
}

echo $posts->content->rendered;